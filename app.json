{
  "expo": {
    "name": "advanced-note-taker",
    "slug": "advanced-note-taker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyunseung1005.advancednotetaker",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "이 앱은 음성 인식을 사용합니다.",
        "NSCameraUsageDescription": "이 앱은 카메라를 사용합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hyunseung1005.advancednotetaker",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "plugins": ["@react-native-voice/voice"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a5f93df8-6c87-452c-95c7-010f2d558b25"
      }
    }
  }
}
